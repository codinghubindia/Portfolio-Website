{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://codinghubindia.github.io/Portfolio-Website",
  "type": "module",
  "scripts": {
    "dev": "vite --host --base=/Portfolio-Website/",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview --base=/Portfolio-Website/",
    "predeploy": "npm run build && node scripts/copy-404.js",
    "deploy": "gh-pages -d dist",
    "clean": "rimraf dist node_modules/.vite",
    "reset": "rimraf dist node_modules package-lock.json && npm install",
    "start:fresh": "npm run reset && npm run dev"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "dotenv": "^16.4.7",
    "framer-motion": "^11.0.5",
    "gsap": "^3.12.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.8.0",
    "split-type": "^0.3.4",
    "typed.js": "^2.0.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.1.3",
    "vite-plugin-compression": "^0.5.1"
  }
}
